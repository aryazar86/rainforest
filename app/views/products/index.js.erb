$('.products-front').append('<%= j render(@products) %>');
$('.pagination').replaceWith('<%= j paginate(@products) %>');

$( ".open-event" ).tooltip({
      show: null,
      position: {
        my: "left top",
        at: "left bottom"
      },
      open: function( event, ui ) {
        ui.tooltip.animate({ top: ui.tooltip.position().top + 10 }, "fast" );
      }
});